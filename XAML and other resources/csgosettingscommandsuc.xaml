<liiil1:dje_zWFW59RU6NNW3CLHAZ6XGYGGPSTQU37RXP4ZJ4GZVENB7K7DXWQTPVLGU7NTPYHS48A_ejd xmlns:liiil1="clr-namespace:-" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:liil1il="http://schemas.microsoft.com/winfx/2006/xaml">
	<Grid
		Margin="0,0,0,5">
		<Grid.RowDefinitions>
			<RowDefinition
				Height="auto" />
		</Grid.RowDefinitions>
		<Grid
			Row="0">
			<Border
				Background="#FF000000"
				CornerRadius="20"
				Opacity="0.6" />
			<StackPanel
				Margin="10">
				<Grid
					VerticalAlignment="Center">
					<Grid.ColumnDefinitions>
						<ColumnDefinition
							Width="*" />
						<ColumnDefinition
							Width="auto" />
						<ColumnDefinition
							Width="auto" />
						<ColumnDefinition
							Width="auto" />
					</Grid.ColumnDefinitions>
					<Grid.RowDefinitions>
						<RowDefinition
							Height="auto" />
						<RowDefinition
							Height="auto" />
						<RowDefinition
							Height="auto" />
					</Grid.RowDefinitions>
					<StackPanel
						Grid.Row="0"
						Grid.Column="0"
						Orientation="Horizontal">
						<CheckBox
							Name="NameCheckBox"
							Foreground="#FFFFFFFF"
							RenderOptions.BitmapScalingMode="Fant"
							Style="{DynamicResource TweaksButtonStyleCanCopy}"
							Content="{Binding Command, Mode=OneWay}" />
					</StackPanel>
					<TextBlock
						Name="StatusTextBlock"
						Grid.Row="0"
						Grid.Column="2"
						Margin="5,0,5,0"
						VerticalAlignment="Center"
						FontSize="15"
						FontWeight="Medium"
						Opacity="0"
						TextAlignment="Right" />
					<StackPanel
						Grid.Row="0"
						Grid.Column="1"
						Orientation="Horizontal">
						<Border
							Grid.Row="0"
							Grid.Column="0"
							Margin="0,0,5,0"
							Background="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z52HQ7LJ6MG7CVYQ_ejd}"
							CornerRadius="5">
							<TextBlock
								Margin="5,0,5,0"
								VerticalAlignment="Center"
								FontSize="14"
								FontWeight="Medium"
								Foreground="#FF000000"
								Text="CL"
								ToolTip="{DynamicResource ClDesc}"
								ToolTipService.BetweenShowDelay="5"
								ToolTipService.InitialShowDelay="5" />
							<FrameworkElement.Style>
								<Style
									TargetType="{liil1il:Type Border}">
									<Style.Triggers>
										<DataTrigger
											Value="False"
											Binding="{Binding IsClient}">
											<Setter
												Property="Visibility"
												Value="Collapsed" />
										</DataTrigger>
									</Style.Triggers>
								</Style>
							</FrameworkElement.Style>
						</Border>
						<Border
							Grid.Row="0"
							Grid.Column="0"
							Margin="0,0,5,0"
							Background="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z2B5DBTEJ9B4VE9Z_ejd}"
							CornerRadius="5">
							<TextBlock
								Margin="5,0,5,0"
								VerticalAlignment="Center"
								FontSize="14"
								FontWeight="Medium"
								Foreground="#FFFFFFFF"
								Text="SV"
								ToolTip="{DynamicResource SvDesc}"
								ToolTipService.BetweenShowDelay="5"
								ToolTipService.InitialShowDelay="5" />
							<FrameworkElement.Style>
								<Style
									TargetType="{liil1il:Type Border}">
									<Style.Triggers>
										<DataTrigger
											Value="False"
											Binding="{Binding IsServer}">
											<Setter
												Property="Visibility"
												Value="Collapsed" />
										</DataTrigger>
									</Style.Triggers>
								</Style>
							</FrameworkElement.Style>
						</Border>
						<Border
							Grid.Row="0"
							Grid.Column="0"
							Margin="0,0,5,0"
							Background="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_zUA9MYQN4ZPV2ETZ_ejd}"
							CornerRadius="5">
							<TextBlock
								Margin="5,0,5,0"
								VerticalAlignment="Center"
								FontSize="14"
								FontWeight="Medium"
								Foreground="#FFFFFFFF"
								Text="CHEAT"
								ToolTip="{DynamicResource CheatDesc}"
								ToolTipService.BetweenShowDelay="5"
								ToolTipService.InitialShowDelay="5" />
							<FrameworkElement.Style>
								<Style
									TargetType="{liil1il:Type Border}">
									<Style.Triggers>
										<DataTrigger
											Value="False"
											Binding="{Binding IsCheat}">
											<Setter
												Property="Visibility"
												Value="Collapsed" />
										</DataTrigger>
									</Style.Triggers>
								</Style>
							</FrameworkElement.Style>
						</Border>
					</StackPanel>
					<StackPanel
						Grid.Row="0"
						Grid.Column="3"
						Orientation="Horizontal">
						<TextBox
							Name="OwnValue"
							Padding="5,0,5,0"
							HorizontalAlignment="Right"
							FontSize="14"
							FontWeight="Medium"
							Foreground="#FF1E1E1E"
							MaxLines="1"
							Template="{StaticResource TextBoxBaseControlTemplateForCS}"
							Text="{DynamicResource EnterOwnValue}"
							TextAlignment="Center" />
						<Button
							Name="DefaultCheckBox"
							Height="24"
							Margin="10,0,0,0"
							HorizontalAlignment="Center"
							VerticalAlignment="Center"
							Content="{DynamicResource AddToAutoexec}"
							Style="{DynamicResource MediumRoundedButtonBlue}" />
					</StackPanel>
					<StackPanel
						Grid.Row="2"
						Grid.Column="0"
						Grid.ColumnSpan="3"
						Margin="10,5,10,0"
						Opacity="0"
						Visibility="Collapsed">
						<StackPanel
							Margin="0,0,0,5"
							Orientation="Horizontal">
							<TextBlock
								Grid.Row="1"
								Grid.Column="0"
								Margin="0,5,5,0"
								VerticalAlignment="Center"
								FontSize="14"
								FontWeight="Medium"
								Foreground="#FFFFFFFF"
								Text="{DynamicResource default}" />
							<TextBox
								Grid.Row="1"
								Grid.Column="1"
								Margin="10,5,5,0"
								Padding="5,2,5,2"
								HorizontalAlignment="Left"
								VerticalAlignment="Center"
								VerticalContentAlignment="Center"
								FontSize="14"
								FontWeight="Medium"
								Foreground="#FFDDDDDD"
								IsReadOnly="True"
								Template="{DynamicResource ComboColoredTextBoxTemplate}"
								TextAlignment="Left"
								TextWrapping="NoWrap"
								Text="{Binding DefaultValue, Mode=OneWay}" />
						</StackPanel>
						<TextBlock
							Grid.Row="1"
							Grid.Column="0"
							Margin="0,5,0,5"
							VerticalAlignment="Center"
							FontSize="14"
							FontWeight="Medium"
							Foreground="#FFFFFFFF"
							Text="{DynamicResource Description}" />
						<TextBlock
							FontSize="14"
							FontWeight="Regular"
							Foreground="#FFDDDDDD"
							TextWrapping="Wrap"
							Text="{Binding Description, Mode=OneWay}" />
						<FrameworkElement.Style>
							<Style
								TargetType="{liil1il:Type StackPanel}">
								<Style.Triggers>
									<DataTrigger
										Value="True"
										Binding="{Binding ElementName=NameCheckBox, Path=IsChecked}">
										<TriggerBase.EnterActions>
											<BeginStoryboard>
												<Storyboard>
													<DoubleAnimation
														BeginTime="0:0:0.2"
														Storyboard.TargetProperty="Opacity"
														From="0"
														To="1"
														Duration="0:0:0.2" />
													<ObjectAnimationUsingKeyFrames
														Storyboard.TargetProperty="Visibility">
														<DiscreteObjectKeyFrame
															KeyTime="0:0:0.02"
															Value="{liil1il:Static Visibility.Visible}" />
													</ObjectAnimationUsingKeyFrames>
												</Storyboard>
											</BeginStoryboard>
										</TriggerBase.EnterActions>
										<TriggerBase.ExitActions>
											<BeginStoryboard>
												<Storyboard>
													<DoubleAnimation
														Storyboard.TargetProperty="Opacity"
														From="1"
														To="0"
														Duration="0:0:0.2" />
													<ObjectAnimationUsingKeyFrames
														BeginTime="0:0:0.2"
														Storyboard.TargetProperty="Visibility">
														<DiscreteObjectKeyFrame
															KeyTime="0:0:0.02"
															Value="{liil1il:Static Visibility.Collapsed}" />
													</ObjectAnimationUsingKeyFrames>
												</Storyboard>
											</BeginStoryboard>
										</TriggerBase.ExitActions>
									</DataTrigger>
								</Style.Triggers>
							</Style>
						</FrameworkElement.Style>
					</StackPanel>
				</Grid>
			</StackPanel>
		</Grid>
	</Grid>
</liiil1:dje_zWFW59RU6NNW3CLHAZ6XGYGGPSTQU37RXP4ZJ4GZVENB7K7DXWQTPVLGU7NTPYHS48A_ejd>
