<liiil1:dje_zTXQ5AHTH6TXHYLF7X8NC3783YSNN9SRAEAENSCT6ECWJUN8PZ2MGBWFTLSGCHK5NRUGTHBGN_ejd xmlns:liiil1="clr-namespace:-" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:li1ii="clr-namespace:Material.Icons.WPF;assembly=Material.Icons.WPF" xmlns:liil1il="http://schemas.microsoft.com/winfx/2006/xaml">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition
				Height="auto" />
			<RowDefinition
				Height="auto" />
		</Grid.RowDefinitions>
		<Border
			Background="#FF000000"
			CornerRadius="20"
			Opacity="0.6" />
		<Grid
			Row="0"
			Margin="20">
			<Grid.ColumnDefinitions>
				<ColumnDefinition
					Width="auto" />
				<ColumnDefinition
					Width="auto" />
				<ColumnDefinition
					Width="*" />
				<ColumnDefinition
					Width="auto" />
			</Grid.ColumnDefinitions>
			<li1ii:MaterialIcon
				Grid.Column="0"
				FrameworkElement.Width="23"
				FrameworkElement.Height="23"
				FrameworkElement.Margin="0,0,5,0"
				Control.Foreground="#FFFFFFFF"
				RenderOptions.BitmapScalingMode="Fant"
				MaterialIcon.Kind="{Binding Icon}" />
			<CheckBox
				Name="NameCheckBox"
				Grid.Column="1"
				Foreground="#FFFFFFFF"
				RenderOptions.BitmapScalingMode="Fant"
				Style="{DynamicResource TweaksButtonStyle}"
				Content="{Binding Name}"
				IsChecked="{Binding ShowDescription, Mode=TwoWay}" />
			<Grid
				Column="2" />
			<Button
				Grid.Column="3"
				Height="30"
				Style="{DynamicResource ApplyButtonStyle}"
				Command="{Binding RunCommand}"
				Visibility="{Binding ApplyVisibility}">
				<ContentControl.Content>
					<StackPanel
						Margin="10,0,10,0"
						Orientation="Horizontal">
						<li1ii:MaterialIcon
							FrameworkElement.Width="15"
							FrameworkElement.Margin="0,0,8,0"
							Control.Foreground="#FFFFFFFF"
							MaterialIcon.Kind="CogPlayOutline" />
						<TextBlock
							HorizontalAlignment="Center"
							VerticalAlignment="Center"
							FontSize="15"
							FontWeight="Regular"
							Foreground="#FFFFFFFF"
							Text="{DynamicResource Run}" />
					</StackPanel>
				</ContentControl.Content>
			</Button>
		</Grid>
		<Grid
			Row="1"
			Margin="0,0,0,20"
			Visibility="Collapsed">
			<Grid.RowDefinitions>
				<RowDefinition
					Height="auto" />
				<RowDefinition
					Height="auto" />
			</Grid.RowDefinitions>
			<TextBlock
				Grid.Row="0"
				Margin="0,0,10,0"
				FontSize="16"
				FontWeight="Regular"
				Foreground="#FFDDDDDD"
				TextWrapping="Wrap"
				Text="{Binding Description}" />
			<TextBox
				Grid.Row="1"
				Margin="0,0,5,0"
				Padding="5,2,5,2"
				HorizontalAlignment="Left"
				VerticalAlignment="Center"
				VerticalContentAlignment="Center"
				FontSize="14"
				FontWeight="Medium"
				Foreground="#FFFFFFFF"
				IsReadOnly="True"
				Template="{DynamicResource ComboColoredTextBoxTemplate}"
				TextAlignment="Left"
				TextWrapping="Wrap"
				Text="{Binding Cmd}" />
			<FrameworkElement.Style>
				<Style
					TargetType="{liil1il:Type Grid}">
					<Style.Triggers>
						<DataTrigger
							Value="True"
							Binding="{Binding ElementName=NameCheckBox, Path=IsChecked}">
							<TriggerBase.EnterActions>
								<BeginStoryboard>
									<Storyboard>
										<DoubleAnimation
											BeginTime="0:0:0.2"
											Storyboard.TargetProperty="Opacity"
											From="0"
											To="1"
											Duration="0:0:0.2" />
										<ObjectAnimationUsingKeyFrames
											Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame
												KeyTime="0:0:0.02"
												Value="{liil1il:Static Visibility.Visible}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</BeginStoryboard>
							</TriggerBase.EnterActions>
							<TriggerBase.ExitActions>
								<BeginStoryboard>
									<Storyboard>
										<DoubleAnimation
											Storyboard.TargetProperty="Opacity"
											From="1"
											To="0"
											Duration="0:0:0.2" />
										<ObjectAnimationUsingKeyFrames
											BeginTime="0:0:0.2"
											Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame
												KeyTime="0:0:0.02"
												Value="{liil1il:Static Visibility.Collapsed}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</BeginStoryboard>
							</TriggerBase.ExitActions>
						</DataTrigger>
					</Style.Triggers>
				</Style>
			</FrameworkElement.Style>
		</Grid>
	</Grid>
</liiil1:dje_zTXQ5AHTH6TXHYLF7X8NC3783YSNN9SRAEAENSCT6ECWJUN8PZ2MGBWFTLSGCHK5NRUGTHBGN_ejd>
