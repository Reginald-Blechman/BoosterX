	<ll1:String
		liil1il:Key="WinServicesProWarningText4">Zalecane dla zaawansowanych użytkowników.</ll1:String>
	<ll1:String
		liil1il:Key="WinServicesProWarningText5">Do pełnego działania na stronie wymagana jest</ll1:String>
