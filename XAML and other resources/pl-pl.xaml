	<ll1:String
		liil1il:Key="Windows-Defender-Default-DefinitionsSimpleDesc">Fabryczne bazy sygnatur Defender</ll1:String>
	<ll1:String
		liil1il:Key="Windows-Defender-Default-DefinitionsDeepDesc">Bez nich ochrona nie działa offline; zaleca się pozostawić</ll1:String>
	<ll1:String
