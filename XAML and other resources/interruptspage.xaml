														Grid.Row="1"
														Margin="10,0,0,0"
														HorizontalAlignment="Left"
														FontSize="18"
														FontWeight="SemiBold"
														Foreground="#FFFFFFFF"
														Text="{Binding DeviceTypeName}" />
												</StackPanel>
												<TextBlock
													Grid.Row="1"
													Margin="0,10,0,0"
													HorizontalAlignment="Left"
													FontSize="18"
													FontWeight="Medium"
													Foreground="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_zRGR5QHU2M7DAPA2_ejd}"
													TextWrapping="Wrap"
													Text="{Binding Name}" />
