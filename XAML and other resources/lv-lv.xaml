	<ll1:String
		liil1il:Key="Windows-Identity-FoundationNuances">Noņem dll System.IdentityModel.Selectors</ll1:String>
	<ll1:String
		liil1il:Key="WindowsMediaPlayerSimpleDesc">Klasiskais WMP 12</ll1:String>
	<ll1:String
