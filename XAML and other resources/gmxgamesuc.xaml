<liiil1:dje_zQYVQXVX6YYVUT5X4D2BLRQY8E6ANZC4J4AVU2NBQS9BRR22E7GM2HQX72SYC57TC2W_ejd xmlns:liiil1="clr-namespace:-" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:liil1il="http://schemas.microsoft.com/winfx/2006/xaml"
	FrameworkElement.Width="164"
	FrameworkElement.Height="200"
	FrameworkElement.Margin="5"
	FrameworkElement.Cursor="Hand"
	ToggleButton.IsChecked="{Binding IsChecked, Mode=TwoWay}">
	<FrameworkElement.Style>
		<Style
			TargetType="{liil1il:Type RadioButton}">
			<Setter
				Property="Template">
				<Setter.Value>
					<ControlTemplate
						TargetType="{liil1il:Type RadioButton}">
						<Grid>
							<Border
								Name="BluredBack"
								Margin="30,0,30,10"
								Background="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_zY99EWG67ABS6GW2_ejd}"
								Opacity="0">
								<UIElement.Effect>
									<BlurEffect
										KernelType="Gaussian"
										Radius="70" />
								</UIElement.Effect>
							</Border>
							<Border
								Background="#FF000000"
								CornerRadius="20"
								Opacity="0.6" />
							<Border
								Name="StrokeColor"
								BorderBrush="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z7Y3AEKDZ_ejd}"
								BorderThickness="2"
								CornerRadius="19"
								Opacity="0" />
							<Border
								Name="BackgroundBorder"
								Width="154"
								Height="190"
								CornerRadius="16">
								<Border.Background>
									<ImageBrush
										Stretch="UniformToFill"
										ImageSource="{Binding Icon}" />
								</Border.Background>
								<UIElement.Clip>
									<RectangleGeometry
										RadiusX="16"
										RadiusY="16"
										Rect="0 0 160 190" />
								</UIElement.Clip>
							</Border>
							<Border
								Width="154"
								Height="190"
								CornerRadius="14">
								<Border.Background>
									<LinearGradientBrush
										StartPoint="0.5,0"
										EndPoint="0.5,1">
										<GradientStop
											Offset="1"
											Color="#FF05070F" />
										<GradientStop
											Offset="0.868"
											Color="#CC05070F" />
										<GradientStop
											Offset="0.601"
											Color="#00000000" />
									</LinearGradientBrush>
								</Border.Background>
							</Border>
							<StackPanel
								Margin="15"
								HorizontalAlignment="Left"
								VerticalAlignment="Bottom"
								Orientation="Vertical">
								<TextBlock
									VerticalAlignment="Center"
									FontSize="16"
									FontWeight="Medium"
									Foreground="#FFFFFFFF"
									TextAlignment="Left"
									TextTrimming="CharacterEllipsis"
									ToolTipService.BetweenShowDelay="5"
									Text="{Binding Name}" />
								<TextBlock
									HorizontalAlignment="Left"
									VerticalAlignment="Center"
									FontSize="14"
									FontWeight="Regular"
									Foreground="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_zRGR5QHU2M7DAPA2_ejd}"
									Opacity="0.8"
									TextAlignment="Left"
									TextTrimming="CharacterEllipsis"
									Text="{Binding FoundIn}" />
							</StackPanel>
						</Grid>
						<ControlTemplate.Triggers>
							<DataTrigger
								Value="True"
								Binding="{Binding IsAdded}">
								<Setter
									Property="IsEnabled"
									Value="False" />
							</DataTrigger>
							<Trigger
								Property="ToggleButton.IsChecked"
								Value="True">
								<TriggerBase.EnterActions>
									<BeginStoryboard>
										<Storyboard>
											<DoubleAnimation
												Storyboard.TargetName="StrokeColor"
												Storyboard.TargetProperty="Opacity"
												To="1"
												Duration="0:0:0.2" />
										</Storyboard>
									</BeginStoryboard>
								</TriggerBase.EnterActions>
								<TriggerBase.ExitActions>
									<BeginStoryboard>
										<Storyboard>
											<DoubleAnimation
												Storyboard.TargetName="StrokeColor"
												Storyboard.TargetProperty="Opacity"
												To="0"
												Duration="0:0:0.2" />
										</Storyboard>
									</BeginStoryboard>
								</TriggerBase.ExitActions>
							</Trigger>
							<EventTrigger
								RoutedEvent="MouseEnter">
								<BeginStoryboard>
									<Storyboard>
										<DoubleAnimation
											Storyboard.TargetName="BluredBack"
											Storyboard.TargetProperty="Opacity"
											To="1"
											Duration="0:0:0.2" />
									</Storyboard>
								</BeginStoryboard>
							</EventTrigger>
							<EventTrigger
								RoutedEvent="MouseLeave">
								<BeginStoryboard>
									<Storyboard>
										<DoubleAnimation
											Storyboard.TargetName="BluredBack"
											Storyboard.TargetProperty="Opacity"
											To="0"
											Duration="0:0:0.2" />
									</Storyboard>
								</BeginStoryboard>
							</EventTrigger>
						</ControlTemplate.Triggers>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>
	</FrameworkElement.Style>
