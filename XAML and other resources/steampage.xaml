									Margin="0,10,0,0"
									HorizontalAlignment="Right"
									Orientation="Horizontal">
									<TextBlock
										Name="AddedTextBlock2"
										Margin="0,0,15,0"
										VerticalAlignment="Center"
										FontFamily="/Resources/Fonts/#LexendExa"
										FontSize="15"
										FontWeight="Bold"
										Foreground="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z7Y3AEKDZ_ejd}"
										Opacity="0"
										Text="{DynamicResource added}"
										TextAlignment="Left" />
									<Button
										Width="150"
										Height="40"
										Content="{DynamicResource Create}"
										Style="{DynamicResource MediumRoundedGradientButton}" />
								</StackPanel>
							</StackPanel>
						</Grid>
					</Grid>
				</Grid>
			</StackPanel>
		</ScrollViewer>
	</Grid>
