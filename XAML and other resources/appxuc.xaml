<liiil1:dje_zLYVYB3NDBF9TV5FWQJXLGFDWW5882T54JCMZDMDC4FZ9YJZMV66VYSDW2P6A_ejd xmlns:liiil1="clr-namespace:-" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:liil1il="http://schemas.microsoft.com/winfx/2006/xaml">
	<CheckBox
		Grid.Row="1"
		Grid.Column="0"
		Width="185"
		Height="197"
		Margin="5"
		Cursor="Hand"
		IsChecked="{Binding AppXIsChecked, Mode=TwoWay}">
		<FrameworkElement.Style>
			<Style
				TargetType="{liil1il:Type CheckBox}">
				<Setter
					Property="Template">
					<Setter.Value>
						<ControlTemplate
							TargetType="{liil1il:Type CheckBox}">
							<Grid>
								<Border
									Name="BluredBack"
									Margin="30,10,30,10"
									Background="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_zY99EWG67ABS6GW2_ejd}"
									Opacity="0">
									<UIElement.Effect>
										<BlurEffect
											KernelType="Gaussian"
											Radius="70" />
									</UIElement.Effect>
								</Border>
								<Border
									Background="#FF000000"
									CornerRadius="20"
									Opacity="0.6" />
								<Border
									Name="MainBorderStroke"
									Width="185"
									Height="197"
									HorizontalAlignment="Center"
									VerticalAlignment="Top"
									BorderBrush="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z7Y3AEKDZ_ejd}"
									BorderThickness="2"
									CornerRadius="18"
									Opacity="0" />
								<StackPanel
									Margin="8">
									<Grid
										Width="169"
										Height="124">
										<Border
											Background="#FF1A2236" />
										<Border
											Width="238"
											Height="159"
											HorizontalAlignment="Center"
											VerticalAlignment="Center"
											CornerRadius="12">
											<Border.Background>
												<ImageBrush
													Stretch="UniformToFill"
													ImageSource="{Binding AppXIconBlur}" />
											</Border.Background>
											<UIElement.Effect>
												<BlurEffect
													KernelType="Gaussian"
													Radius="70" />
											</UIElement.Effect>
										</Border>
										<Border
											Background="#FF000000"
											Opacity="0.1" />
										<Border
											Width="76"
											Height="76">
											<Border.Background>
												<ImageBrush
													Stretch="UniformToFill"
													ImageSource="{Binding AppXIcon}" />
											</Border.Background>
											<UIElement.Clip>
												<RectangleGeometry
													RadiusX="12"
													RadiusY="12"
													Rect="0,0,76,76" />
											</UIElement.Clip>
										</Border>
										<UIElement.Clip>
											<RectangleGeometry
												RadiusX="16"
												RadiusY="16"
												Rect="0,0,169,124" />
										</UIElement.Clip>
									</Grid>
									<TextBlock
										Margin="5,8,5,0"
										VerticalAlignment="Center"
										FontSize="15"
										FontWeight="SemiBold"
										Foreground="#FFFFFFFF"
										TextAlignment="Left"
										TextTrimming="CharacterEllipsis"
										ToolTipService.BetweenShowDelay="5"
										Text="{Binding AppXName}"
										ToolTip="{Binding AppXName}" />
									<Grid
										Margin="5,4,5,0">
										<TextBlock
											Margin="0,0,0,0"
											HorizontalAlignment="Right"
											VerticalAlignment="Center"
											FontSize="14"
											FontWeight="Regular"
											Foreground="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_zBHWQM3TLFWTMTC2_ejd}"
											Opacity="0.8"
											TextAlignment="Right"
											TextTrimming="CharacterEllipsis"
											Text="{Binding Information}" />
										<StackPanel
											HorizontalAlignment="Left"
											VerticalAlignment="Center"
											Orientation="Horizontal">
											<Image
												Width="14"
												Source="{DynamicResource StarSvg}" />
											<TextBlock
												Margin="2,0,0,0"
												HorizontalAlignment="Right"
												VerticalAlignment="Center"
												FontSize="14"
												FontWeight="Regular"
												Foreground="#FFFFFFFF"
												TextAlignment="Right"
												TextTrimming="CharacterEllipsis"
												Text="{Binding AppXAverageRating}" />
										</StackPanel>
									</Grid>
								</StackPanel>
							</Grid>
							<ControlTemplate.Triggers>
								<Trigger
									Property="ToggleButton.IsChecked"
									Value="True">
									<TriggerBase.EnterActions>
										<BeginStoryboard>
											<Storyboard>
												<DoubleAnimation
													Storyboard.TargetName="MainBorderStroke"
													Storyboard.TargetProperty="Opacity"
													To="1"
													Duration="0:0:0.2" />
												<DoubleAnimation
													Storyboard.TargetName="BluredBack"
													Storyboard.TargetProperty="Opacity"
													To="1"
													Duration="0:0:0.2" />
											</Storyboard>
										</BeginStoryboard>
									</TriggerBase.EnterActions>
									<TriggerBase.ExitActions>
										<BeginStoryboard>
											<Storyboard>
												<DoubleAnimation
													Storyboard.TargetName="MainBorderStroke"
													Storyboard.TargetProperty="Opacity"
													To="0"
													Duration="0:0:0.2" />
												<DoubleAnimation
													Storyboard.TargetName="BluredBack"
													Storyboard.TargetProperty="Opacity"
													To="0"
													Duration="0:0:0.2" />
											</Storyboard>
										</BeginStoryboard>
									</TriggerBase.ExitActions>
								</Trigger>
							</ControlTemplate.Triggers>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style>
		</FrameworkElement.Style>
	</CheckBox>
</liiil1:dje_zLYVYB3NDBF9TV5FWQJXLGFDWW5882T54JCMZDMDC4FZ9YJZMV66VYSDW2P6A_ejd>
