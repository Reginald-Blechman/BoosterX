<liiil1:dje_zX78EUVM3MQT4EM6QFB5C6FNQ3L7EEQRZTGAXDZ5MCWJBWW2PSVEJE_ejd xmlns:liiil1="clr-namespace:-" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:liil1il="http://schemas.microsoft.com/winfx/2006/xaml">
	<Grid>
		<Border
			Width="275"
			Height="95">
			<StackPanel
				Grid.Row="0"
				Grid.Column="0"
				Grid.ColumnSpan="2"
				HorizontalAlignment="Center"
				VerticalAlignment="Center">
				<Border
					Name="superborder"
					Width="270"
					Height="90"
					HorizontalAlignment="Center"
					VerticalAlignment="Center"
					BorderThickness="4"
					CornerRadius="16">
					<Border.BorderBrush>
						<RadialGradientBrush
							Center="0.5,0.5"
							GradientOrigin="0.5,0.5"
							MappingMode="RelativeToBoundingBox"
							Opacity="0"
							RadiusX="0.5"
							SpreadMethod="Pad">
							<GradientStop
								Color="White" />
							<GradientStop
								Offset="1"
								Color="#FF848484" />
						</RadialGradientBrush>
					</Border.BorderBrush>
					<Button
						Name="Startoptim"
						Width="254"
						Height="74"
						HorizontalAlignment="Center"
						VerticalAlignment="Center"
						Cursor="Hand">
						<UIElement.Effect>
							<DropShadowEffect
								liil1il:Name="btnshadow"
								BlurRadius="0"
								Opacity="1"
								RenderingBias="Performance"
								ShadowDepth="0"
								Color="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_zD9GRSSAL8RCY2D2_ejd}" />
						</UIElement.Effect>
						<FrameworkElement.Resources>
							<ResourceDictionary>
								<Style
									TargetType="{liil1il:Type Border}"
									liil1il:Key="{liil1il:Type Border}">
									<Setter
										Property="Border.CornerRadius"
										Value="12" />
								</Style>
								<Style
									TargetType="{liil1il:Type Button}"
									liil1il:Key="{liil1il:Type Button}">
									<Setter
										Property="Background"
										Value="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z7Y3AEKDZ_ejd}" />
									<Setter
										Property="Template">
										<Setter.Value>
											<ControlTemplate
												TargetType="{liil1il:Type Button}">
												<Border
													Background="{TemplateBinding Background}">
													<ContentPresenter
														HorizontalAlignment="Center"
														VerticalAlignment="Center" />
												</Border>
											</ControlTemplate>
										</Setter.Value>
									</Setter>
									<Style.Triggers>
										<Trigger
											Property="UIElement.IsMouseOver"
											Value="True">
											<Setter
												Property="Background"
												Value="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_zRGR5QHU2M7DAPA2_ejd}" />
										</Trigger>
										<Trigger
											Property="IsPressed"
											Value="True">
											<Setter
												Property="Background"
												Value="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_zBHWQM3TLFWTMTC2_ejd}" />
										</Trigger>
									</Style.Triggers>
								</Style>
							</ResourceDictionary>
						</FrameworkElement.Resources>
						<FrameworkElement.Triggers>
							<EventTrigger
								RoutedEvent="Button.MouseEnter">
								<BeginStoryboard>
									<Storyboard>
										<DoubleAnimation
											DesiredFrameRate="24"
											Storyboard.TargetName="superborder"
											Storyboard.TargetProperty="(Border.BorderBrush).Opacity"
											From="0"
											To="0.9"
											Duration="0:0:0.2" />
										<DoubleAnimation
											DesiredFrameRate="24"
											Storyboard.TargetName="superborder"
											Storyboard.TargetProperty="Width"
											From="270"
											To="275"
											Duration="0:0:0.1" />
										<DoubleAnimation
											DesiredFrameRate="24"
											Storyboard.TargetName="superborder"
											Storyboard.TargetProperty="Height"
											From="90"
											To="95"
											Duration="0:0:0.1" />
										<DoubleAnimation
											DesiredFrameRate="24"
											Storyboard.TargetName="Startoptim"
											Storyboard.TargetProperty="Width"
											From="254"
											To="259"
											Duration="0:0:0.1" />
										<DoubleAnimation
											DesiredFrameRate="24"
											Storyboard.TargetName="Startoptim"
											Storyboard.TargetProperty="Height"
											From="74"
											To="79"
											Duration="0:0:0.1" />
										<DoubleAnimation
											BeginTime="0:0:0.1"
											DesiredFrameRate="24"
											Storyboard.TargetName="superborder"
											Storyboard.TargetProperty="Width"
											From="275"
											To="270"
											Duration="0:0:0.2" />
										<DoubleAnimation
											BeginTime="0:0:0.1"
											DesiredFrameRate="24"
											Storyboard.TargetName="superborder"
											Storyboard.TargetProperty="Height"
											From="95"
											To="90"
											Duration="0:0:0.2" />
										<DoubleAnimation
											BeginTime="0:0:0.1"
											DesiredFrameRate="24"
											Storyboard.TargetName="Startoptim"
											Storyboard.TargetProperty="Width"
											From="259"
											To="254"
											Duration="0:0:0.2" />
										<DoubleAnimation
											BeginTime="0:0:0.1"
											DesiredFrameRate="24"
											Storyboard.TargetName="Startoptim"
											Storyboard.TargetProperty="Height"
											From="79"
											To="74"
											Duration="0:0:0.2" />
										<DoubleAnimation
											DesiredFrameRate="24"
											Storyboard.TargetName="btnshadow"
											Storyboard.TargetProperty="BlurRadius"
											From="0"
											To="50"
											Duration="0:0:0.1" />
										<DoubleAnimation
											BeginTime="0:0:0.1"
											DesiredFrameRate="24"
											Storyboard.TargetName="btnshadow"
											Storyboard.TargetProperty="BlurRadius"
											From="40"
											To="20"
											Duration="0:0:0.25" />
										<DoubleAnimation
											DesiredFrameRate="24"
											Storyboard.TargetName="btnshadow"
											Storyboard.TargetProperty="Opacity"
											From="0"
											To="1"
											Duration="0:0:0.0005" />
									</Storyboard>
								</BeginStoryboard>
							</EventTrigger>
							<EventTrigger
								RoutedEvent="Button.MouseLeave">
								<BeginStoryboard>
									<Storyboard>
										<DoubleAnimation
											DesiredFrameRate="24"
											Storyboard.TargetName="superborder"
											Storyboard.TargetProperty="(Border.BorderBrush).Opacity"
											From="0.9"
											To="0"
											Duration="0:0:0.3" />
										<DoubleAnimation
											DesiredFrameRate="24"
											Storyboard.TargetName="btnshadow"
											Storyboard.TargetProperty="BlurRadius"
											From="20"
											To="0"
											Duration="0:0:0.3" />
										<DoubleAnimation
											DesiredFrameRate="24"
											Storyboard.TargetName="btnshadow"
											Storyboard.TargetProperty="Opacity"
											From="1"
											To="0"
											Duration="0:0:0.3" />
									</Storyboard>
								</BeginStoryboard>
							</EventTrigger>
						</FrameworkElement.Triggers>
						<TextBlock
							MaxWidth="220"
							HorizontalAlignment="Center"
							VerticalAlignment="Center"
							FontSize="20"
							FontWeight="SemiBold"
							Foreground="#FFFFFFFF"
							Text="{DynamicResource beginopt}"
							TextAlignment="Center"
							TextWrapping="Wrap" />
					</Button>
				</Border>
			</StackPanel>
		</Border>
	</Grid>
</liiil1:dje_zX78EUVM3MQT4EM6QFB5C6FNQ3L7EEQRZTGAXDZ5MCWJBWW2PSVEJE_ejd>
