	<ll1:String
		liil1il:Key="MultiPoint-Connector-ServicesRecommendation">Désactiver</ll1:String>
	<ll1:String
		liil1il:Key="MultiPoint-Connector-ServicesNuances">Économise 10 Mo de RAM</ll1:String>
	<ll1:String
		liil1il:Key="MultiPoint-ToolsSimpleDesc">Console GUI MultiPoint Manager</ll1:String>
	<ll1:String
