<liiil1:dje_zNSNRFVQUMVAUHL755A6AUDYMMVMZMCDHDE3DWVBSGNN9KGA_ejd xmlns:liiil1="clr-namespace:-" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:li1ii="clr-namespace:Material.Icons.WPF;assembly=Material.Icons.WPF" xmlns:liil1il="http://schemas.microsoft.com/winfx/2006/xaml"
	Page.Title="CustomizationMainPage">
	<Grid
		Margin="0,0,20,20">
		<Grid.ColumnDefinitions>
			<ColumnDefinition
				Width="250" />
			<ColumnDefinition
				Width="*" />
		</Grid.ColumnDefinitions>
		<Grid.RowDefinitions>
			<RowDefinition
				Height="75" />
			<RowDefinition
				Height="*" />
		</Grid.RowDefinitions>
		<StackPanel
			Grid.Row="0"
			Grid.Column="0"
			Margin="10,10,0,10"
			VerticalAlignment="Bottom"
			Orientation="Horizontal">
			<Button
				Name="BackButton"
				Width="30"
				Height="30"
				Margin="0,0,10,0"
				Style="{DynamicResource BorderOutlineButtonStyle}"
				ToolTip="{DynamicResource Back}"
				ToolTipService.BetweenShowDelay="500"
				ToolTipService.InitialShowDelay="500"
				Visibility="Collapsed">
				<li1ii:MaterialIcon
					FrameworkElement.Width="25"
					FrameworkElement.Height="25"
					Control.Foreground="#FFFFFFFF"
					MaterialIcon.Kind="ArrowLeft"
					RenderOptions.BitmapScalingMode="Fant" />
			</Button>
			<TextBlock
				FontSize="22"
				FontWeight="SemiBold"
				Foreground="#FFFFFFFF"
				Text="{DynamicResource Optimization}"
				TextAlignment="Left" />
			<Button
				Width="15"
				Margin="10,0,0,0"
				ClipToBounds="False"
				Style="{DynamicResource InvisibleButton}"
				ToolTip="{DynamicResource RevertAll}"
				ToolTipService.InitialShowDelay="50"
				Visibility="{Binding Everything}">
				<Image
					Source="{StaticResource RollbackDrawingImage}" />
			</Button>
		</StackPanel>
		<StackPanel
			Grid.Row="0"
			Grid.Column="1"
			Margin="20,10,0,10"
			VerticalAlignment="Bottom"
			Orientation="Horizontal">
			<TextBlock
				Name="PageTitle"
				VerticalAlignment="Center"
				FontSize="20"
				FontWeight="Regular"
				Foreground="#FFFFFFFF"
				TextAlignment="Left" />
			<Image
				Name="ProImage"
				Height="20"
				Margin="10,0,0,0"
				VerticalAlignment="Center"
				Opacity="0"
				Source="pack://application:,,,/BoosterX;component/Resources\Images\ProLabel.png" />
		</StackPanel>
		<Grid
			Row="1"
			Column="0">
			<Grid.ColumnDefinitions>
				<ColumnDefinition
					Width="*" />
			</Grid.ColumnDefinitions>
			<Grid.RowDefinitions>
				<RowDefinition
					Height="*" />
				<RowDefinition
					Height="auto" />
				<RowDefinition
					Height="auto" />
			</Grid.RowDefinitions>
			<Grid
				Row="0"
				Column="0">
				<Border
					Background="#FF000000"
					CornerRadius="20"
					Opacity="0.6" />
				<ScrollViewer
					Name="MenuScroll"
					Margin="10,10,0,10"
					HorizontalScrollBarVisibility="Disabled"
					VerticalScrollBarVisibility="Auto">
					<Grid>
						<StackPanel>
							<RadioButton
								Name="PowerButton"
								Width="200"
								Height="35"
								Margin="0,0,0,5"
								HorizontalAlignment="Left"
								GroupName="OptimizationButtons"
								Style="{StaticResource TweakingButtonStyleViolet}">
								<ContentControl.Content>
									<StackPanel
										Margin="10,0,10,0"
										Orientation="Horizontal">
										<Image
											Width="18"
											Height="18"
											RenderOptions.BitmapScalingMode="Fant"
											Source="{DynamicResource PowerSvg}" />
										<TextBlock
											Margin="8,0,0,0"
											VerticalAlignment="Center"
											FontSize="17"
											FontWeight="Regular"
											Foreground="#FFFFFFFF"
											Text="{DynamicResource powermgmtexp}" />
									</StackPanel>
								</ContentControl.Content>
							</RadioButton>
						</StackPanel>
						<StackPanel
							Name="ChangesStackPanel"
							Margin="10,0,10,10"
							HorizontalAlignment="Right">
							<TextBlock
								Style="{DynamicResource OptCountTextBlockStyle}"
								Text="{Binding PowerSettings}" />
						</StackPanel>
					</Grid>
				</ScrollViewer>
			</Grid>
			<Grid
				Name="BorderForSections"
				Row="0"
				Column="0"
				Opacity="0"
				Visibility="Collapsed">
				<Border
					Background="#00FFFFFF"
					BorderBrush="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_zRGR5QHU2M7DAPA2_ejd}"
					BorderThickness="2"
					CornerRadius="20" />
			</Grid>
			<Button
				Name="ApplyButton"
				Grid.Row="2"
				Grid.Column="0"
				Height="45"
				Margin="0,20,0,0"
				VerticalAlignment="Bottom"
				Style="{DynamicResource ApplyButtonStyle}">
				<ContentControl.Content>
					<StackPanel
						Orientation="Horizontal">
						<Image
							Width="20"
							Margin="0,0,8,0"
							Source="{StaticResource ApplyFilled}" />
						<TextBlock
							HorizontalAlignment="Center"
							VerticalAlignment="Center"
							FontSize="20"
							FontWeight="Regular"
							Foreground="#FFFFFFFF"
							Text="{DynamicResource apply}" />
					</StackPanel>
				</ContentControl.Content>
			</Button>
		</Grid>
		<Frame
			Name="CustomizationFrame"
			Grid.Row="1"
			Grid.Column="1"
			Margin="15,0,0,0"
			NavigationUIVisibility="Hidden" />
	</Grid>
</liiil1:dje_zNSNRFVQUMVAUHL755A6AUDYMMVMZMCDHDE3DWVBSGNN9KGA_ejd>
