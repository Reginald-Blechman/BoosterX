<liiil1:dje_zYKWNAHPAA2WHHRJF4X7EG3NQ8TJT6P4HVXCZBEX5BLD2JKUFM4PCTXZ75CCLQQGATQ_ejd xmlns:liiil1="clr-namespace:-" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:liil1il="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:li1ii="clr-namespace:Material.Icons.WPF;assembly=Material.Icons.WPF"
	Page.Title="NetworkAdapterPage">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition
				Height="auto" />
			<RowDefinition
				Height="auto" />
			<RowDefinition
				Height="*" />
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition
				Width="*" />
		</Grid.ColumnDefinitions>
		<Grid
			Row="0"
			Column="0"
			Width="625"
			HorizontalAlignment="Left">
			<Border
				Background="#FF000000"
				CornerRadius="20"
				Opacity="0.6" />
			<StackPanel
				Margin="6.5">
				<StackPanel
					Margin="0,0,0,0"
					HorizontalAlignment="Stretch"
					VerticalAlignment="Center"
					Orientation="Horizontal">
					<Button
						Name="RevertButton"
						Width="150"
						Height="36"
						Margin="0,0,0,0"
						Background="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z2B5DBTEJ9B4VE9Z_ejd}"
						Style="{DynamicResource TweakingTopButton}"
						ToolTip="{DynamicResource NetworkAdaptersDefDesc}"
						ToolTipService.BetweenShowDelay="0"
						ToolTipService.InitialShowDelay="0"
						Command="{Binding DefaultCommand}">
						<ContentControl.Content>
							<StackPanel
								Orientation="Horizontal">
								<Image
									Width="15"
									Margin="0,0,8,0"
									Source="{StaticResource SpeedometerLow}" />
								<TextBlock
									HorizontalAlignment="Center"
									VerticalAlignment="Center"
									FontSize="16"
									FontWeight="Regular"
									Foreground="#FFFFFFFF"
									Text="{DynamicResource default}" />
							</StackPanel>
						</ContentControl.Content>
					</Button>
					<Button
						Name="OptimalButton"
						Width="150"
						Height="36"
						Margin="4,0,0,0"
						Background="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z7Y3AEKDZ_ejd}"
						Style="{DynamicResource TweakingTopButtonPRO}"
						ToolTip="{DynamicResource OptimalAdapterDesc}"
						ToolTipService.BetweenShowDelay="0"
						ToolTipService.InitialShowDelay="0"
						Command="{Binding OptimalClickCommand}">
						<ContentControl.Content>
							<StackPanel
								Orientation="Horizontal">
								<Image
									Width="15"
									Margin="0,0,8,0"
									Source="{StaticResource SpeedometerMiddle}" />
								<TextBlock
									HorizontalAlignment="Center"
									VerticalAlignment="Center"
									FontSize="16"
									FontWeight="Regular"
									Foreground="#FFFFFFFF"
									Text="{DynamicResource optimal}" />
							</StackPanel>
						</ContentControl.Content>
					</Button>
					<Button
						Name="UltraLowLatencyButton"
						Width="150"
						Height="36"
						Margin="4,0,0,0"
						Background="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_zUA9MYQN4ZPV2ETZ_ejd}"
						Style="{DynamicResource TweakingTopButtonPRO}"
						ToolTip="{DynamicResource UltraLowLatencyAdapterDesc}"
						ToolTipService.BetweenShowDelay="0"
						ToolTipService.InitialShowDelay="0"
						Command="{Binding MaximumClickCommand}">
						<ContentControl.Content>
							<StackPanel
								Orientation="Horizontal">
								<Image
									Width="15"
									Margin="0,0,8,0"
									Source="{StaticResource SpeedometerMax}" />
								<TextBlock
									HorizontalAlignment="Center"
									VerticalAlignment="Center"
									FontSize="16"
									FontWeight="Regular"
									Foreground="#FFFFFFFF"
									Text="Low Latency" />
							</StackPanel>
						</ContentControl.Content>
					</Button>
					<Button
						Name="RefreshButton"
						Width="150"
						Height="36"
						Margin="4,0,0,0"
						HorizontalAlignment="Right"
						Background="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z2B5DBTEJ9B4VE9Z_ejd}"
						Style="{DynamicResource TweakingTopButton}"
						Command="{Binding UpdateCommand}">
						<ContentControl.Content>
							<StackPanel
								Orientation="Horizontal">
								<Image
									Width="15"
									Margin="0,0,8,0"
									Source="{StaticResource RefreshIcon}" />
								<TextBlock
									HorizontalAlignment="Center"
									VerticalAlignment="Center"
									FontSize="16"
									FontWeight="Regular"
									Foreground="#FFFFFFFF"
									Text="{DynamicResource update}" />
							</StackPanel>
						</ContentControl.Content>
					</Button>
				</StackPanel>
				<Grid
					Margin="0,6.5,0,0">
					<StackPanel>
						<Grid
							VerticalAlignment="Center">
							<Grid.ColumnDefinitions>
								<ColumnDefinition
									Width="auto" />
								<ColumnDefinition
									Width="*" />
							</Grid.ColumnDefinitions>
							<Grid
								Height="36"
								Margin="0,0,5,0">
								<ComboBox
									HorizontalAlignment="Stretch"
									VerticalAlignment="Center"
									ItemContainerStyle="{DynamicResource theComboBoxItem}"
									Style="{DynamicResource theComboBoxServices}"
									ItemsSource="{Binding NetworkAdapters}"
									SelectedItem="{Binding CurrentNetworkAdapter, Mode=TwoWay}">
									<ItemsControl.ItemTemplate>
										<DataTemplate>
											<StackPanel
												Orientation="Horizontal">
												<li1ii:MaterialIcon
													FrameworkElement.Width="23"
													FrameworkElement.Height="23"
													FrameworkElement.Margin="0,0,6,0"
													Control.Foreground="#FFFFFFFF"
													RenderOptions.BitmapScalingMode="Fant"
													MaterialIcon.Kind="{Binding Icon}" />
												<TextBlock
													Margin="0,0,0,0"
													HorizontalAlignment="Left"
													VerticalAlignment="Center"
													FontSize="16"
													FontWeight="Regular"
													Foreground="#FFFFFFFF"
													TextTrimming="CharacterEllipsis"
													Text="{Binding Name}" />
											</StackPanel>
										</DataTemplate>
									</ItemsControl.ItemTemplate>
								</ComboBox>
							</Grid>
						</Grid>
					</StackPanel>
				</Grid>
			</StackPanel>
		</Grid>
		<ScrollViewer
			Grid.Row="2"
			Margin="0,10,0,0"
			VerticalScrollBarVisibility="Auto"
			VirtualizingPanel.ScrollUnit="Pixel"
			VirtualizingPanel.VirtualizationMode="Standard">
			<StackPanel>
				<StackPanel
					Grid.Row="0">
					<StackPanel
						HorizontalAlignment="Left"
						Orientation="Horizontal">
						<CheckBox
							Name="TipCheckBox"
							Content="{DynamicResource Tip}"
							Style="{DynamicResource TipCheckBoxStyle}" />
					</StackPanel>
					<StackPanel
						Margin="10,5,10,10"
						Opacity="0"
						Visibility="Collapsed">
						<StackPanel
							Orientation="Horizontal">
							<TextBlock
								HorizontalAlignment="Center"
								VerticalAlignment="Center"
								FontSize="16"
								FontWeight="SemiBold"
								Foreground="#FFFFFFFF"
								Text="{DynamicResource RecommendedToRead}" />
							<TextBlock
								Margin="5,0,0,0"
								HorizontalAlignment="Center"
								VerticalAlignment="Center"
								FontSize="16"
								FontWeight="SemiBold"
								Foreground="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z7Y3AEKDZ_ejd}"
								Text="{DynamicResource description}" />
						</StackPanel>
						<TextBlock
							Margin="0,10,0,0"
							HorizontalAlignment="Left"
							VerticalAlignment="Center"
							FontSize="14"
							FontWeight="Regular"
							Foreground="#FFFFFFFF"
							Opacity="0.9"
							Text="{DynamicResource NetworkAdapterSettingsPageDesc}"
							TextWrapping="Wrap" />
						<FrameworkElement.Style>
							<Style
								TargetType="{liil1il:Type StackPanel}">
								<Style.Triggers>
									<DataTrigger
										Value="True"
										Binding="{Binding ElementName=TipCheckBox, Path=IsChecked}">
										<TriggerBase.EnterActions>
											<BeginStoryboard>
												<Storyboard>
													<DoubleAnimation
														BeginTime="0:0:0.2"
														Storyboard.TargetProperty="Opacity"
														From="0"
														To="1"
														Duration="0:0:0.2" />
													<ObjectAnimationUsingKeyFrames
														Storyboard.TargetProperty="Visibility">
														<DiscreteObjectKeyFrame
															KeyTime="0:0:0.02"
															Value="{liil1il:Static Visibility.Visible}" />
													</ObjectAnimationUsingKeyFrames>
												</Storyboard>
											</BeginStoryboard>
										</TriggerBase.EnterActions>
										<TriggerBase.ExitActions>
											<BeginStoryboard>
												<Storyboard>
													<DoubleAnimation
														Storyboard.TargetProperty="Opacity"
														From="1"
														To="0"
														Duration="0:0:0.2" />
													<ObjectAnimationUsingKeyFrames
														BeginTime="0:0:0.2"
														Storyboard.TargetProperty="Visibility">
														<DiscreteObjectKeyFrame
															KeyTime="0:0:0.02"
															Value="{liil1il:Static Visibility.Collapsed}" />
													</ObjectAnimationUsingKeyFrames>
												</Storyboard>
											</BeginStoryboard>
										</TriggerBase.ExitActions>
									</DataTrigger>
								</Style.Triggers>
							</Style>
						</FrameworkElement.Style>
					</StackPanel>
				</StackPanel>
				<ItemsControl
					Margin="0,10,0,0"
					ItemsSource="{Binding CurrentNetworkAdapter.Settings}">
					<ItemsControl.ItemTemplate>
						<DataTemplate>
							<liiil1:dje_zNRCL9G4XRL4UW4YVEN9US6K5RDEPGK59QWGV6J84YKWMTDVDQY9PKKTRHCFPYAXKMFY55QN2_ejd
								FrameworkElement.DataContext="{Binding}" />
						</DataTemplate>
					</ItemsControl.ItemTemplate>
				</ItemsControl>
			</StackPanel>
		</ScrollViewer>
	</Grid>
</liiil1:dje_zYKWNAHPAA2WHHRJF4X7EG3NQ8TJT6P4HVXCZBEX5BLD2JKUFM4PCTXZ75CCLQQGATQ_ejd>
