							Column="1"
							Margin="10,0,20,0"
							VerticalAlignment="Center">
							<StackPanel>
								<TextBlock
									Grid.Row="0"
									Grid.Column="0"
									Margin="10,5,0,0"
									VerticalAlignment="Center"
									FontFamily="/Resources/Fonts/#Lexend Exa"
									FontSize="22"
									FontWeight="ExtraBlack"
									Foreground="#FFFFFFFF"
									Text="{DynamicResource StrippedSteam}"
									TextAlignment="Left" />
								<Grid
									Margin="0,10,0,0">
									<Border
										Background="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z7Y3AEKDZ_ejd}"
