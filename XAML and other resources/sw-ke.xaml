	<ll1:String
		liil1il:Key="StrippedSteam">STEAM ILIOPUNGUZWA</ll1:String>
	<ll1:String
		liil1il:Key="GamesOptimization">Uboreshaji wa michezo</ll1:String>
	<ll1:String
