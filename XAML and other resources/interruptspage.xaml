							Margin="2,10,0,0"
							ItemsSource="{Binding DeviceViewModels}">
							<ItemsControl.ItemsPanel>
								<ItemsPanelTemplate>
									<WrapPanel
										Orientation="Horizontal" />
								</ItemsPanelTemplate>
							</ItemsControl.ItemsPanel>
							<ItemsControl.ItemTemplate>
								<DataTemplate>
									<RadioButton
										Width="250"
										Height="auto"
										Margin="0,0,10,10"
