<liiil1:dje_zDNJTUR2U6XYFEDXJ3P92ZK2QXYD473YFCNATNSQLHTLREAPRBDBPPE2XCZACBDEZHS_ejd xmlns:liiil1="clr-namespace:-" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:liil1il="http://schemas.microsoft.com/winfx/2006/xaml">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition
				Height="auto" />
			<RowDefinition
				Height="*" />
		</Grid.RowDefinitions>
		<Border
			Grid.RowSpan="2"
			Width="auto"
			Height="auto"
			Background="#FF000000"
			CornerRadius="20"
			Opacity="0.6" />
		<Grid
			Name="NewsGrid"
			Margin="20,20,20,0">
			<StackPanel
				Orientation="Horizontal">
				<TextBlock
					HorizontalAlignment="Left"
					VerticalAlignment="Center"
					FontSize="18"
					FontWeight="Medium"
					Foreground="#FFFFFFFF"
					Text="{DynamicResource News}" />
				<TextBlock
					Margin="10,0,0,0"
					HorizontalAlignment="Left"
					VerticalAlignment="Center"
					FontSize="18"
					FontWeight="Medium"
					Foreground="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_zUA9MYQN4ZPV2ETZ_ejd}"
					Text="{Binding Error}" />
			</StackPanel>
		</Grid>
		<ScrollViewer
			Grid.Row="1"
			Margin="20,8,10,20">
			<ItemsControl
				ItemsSource="{Binding News}">
				<ItemsControl.ItemTemplate>
					<DataTemplate>
						<StackPanel
							HorizontalAlignment="Left"
							VerticalAlignment="Top"
							Orientation="Vertical">
							<Border
								Height="30"
								VerticalAlignment="Center"
								Background="#FF0A1128"
								CornerRadius="15">
								<StackPanel
									Margin="10,0,10,0"
									VerticalAlignment="Center"
									Orientation="Horizontal">
									<TextBlock
										FontSize="14"
										FontWeight="Medium"
										Foreground="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_zRGR5QHU2M7DAPA2_ejd}"
										Opacity="0.7"
										Text="{Binding DateCreated}" />
									<TextBlock
										Margin="10,0,0,0"
										FontSize="14"
										FontWeight="Medium"
										Foreground="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_zRGR5QHU2M7DAPA2_ejd}"
										Opacity="0.7"
										Text="{Binding Creator}" />
									<TextBlock
										Margin="10,0,0,0"
										FontSize="14"
										FontWeight="Medium"
										Foreground="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z7Y3AEKDZ_ejd}"
										Text="{Binding Name}" />
								</StackPanel>
							</Border>
							<StackPanel
								Margin="0,6,5,10">
								<TextBlock
									FontSize="15"
									FontWeight="Regular"
									Opacity="0.6"
									TextAlignment="Left"
									TextWrapping="Wrap"
									Foreground="{Binding Color}"
									Text="{Binding Content}" />
							</StackPanel>
							<ItemsControl
								Margin="0,0,0,10"
								ItemsSource="{Binding Buttons}"
								Visibility="{Binding ButtonsVisibility}">
								<ItemsControl.ItemsPanel>
									<ItemsPanelTemplate>
										<StackPanel
											HorizontalAlignment="Left"
											VerticalAlignment="Top"
											Orientation="Horizontal" />
									</ItemsPanelTemplate>
								</ItemsControl.ItemsPanel>
								<ItemsControl.ItemTemplate>
									<DataTemplate>
										<Button
											Margin="0,0,10,0"
											Command="{Binding Command}"
											Content="{Binding Content}"
											Style="{Binding Style}" />
									</DataTemplate>
								</ItemsControl.ItemTemplate>
							</ItemsControl>
						</StackPanel>
					</DataTemplate>
				</ItemsControl.ItemTemplate>
			</ItemsControl>
		</ScrollViewer>
	</Grid>
</liiil1:dje_zDNJTUR2U6XYFEDXJ3P92ZK2QXYD473YFCNATNSQLHTLREAPRBDBPPE2XCZACBDEZHS_ejd>
