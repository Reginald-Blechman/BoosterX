	<ll1:String
		liil1il:Key="Only2Keys">Một lần nữa, chỉ 2 phím</ll1:String>
	<ll1:String
		liil1il:Key="BasicAITip">Nhanh nhất, ổn định. 8B</ll1:String>
	<ll1:String
		liil1il:Key="ProAITip">Thông minh, ổn định. 14B</ll1:String>
