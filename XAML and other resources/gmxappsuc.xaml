<liiil1:dje_zXNNJLKZGU3XN9MK85ZPHRMHN6TLT9JSR6D4D7NR9PZJCSNHLEEUA4V5W85KFULPDMQ_ejd xmlns:liiil1="clr-namespace:-" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:liil1il="http://schemas.microsoft.com/winfx/2006/xaml">
	<Button
		Height="60"
		Margin="5"
		Cursor="Hand"
		Command="{Binding DeleteCommand}">
		<FrameworkElement.Style>
			<Style
				BasedOn="{StaticResource InvisibleButton}"
				TargetType="{liil1il:Type Button}">
				<Setter
					Property="Template">
					<Setter.Value>
						<ControlTemplate
							TargetType="{liil1il:Type Button}">
							<Grid>
								<Grid.ColumnDefinitions>
									<ColumnDefinition
										Width="*" />
									<ColumnDefinition
										Width="auto" />
								</Grid.ColumnDefinitions>
								<Border
									Grid.Column="0"
									Grid.ColumnSpan="2"
									Background="#FF000000"
									CornerRadius="20"
									Opacity="0.6" />
								<StackPanel
									Grid.Column="0"
									Margin="5"
									Orientation="Horizontal">
									<Border
										Name="BackgroundBorder"
										Width="50"
										Height="50"
										CornerRadius="16">
										<Border.Background>
											<ImageBrush
												Stretch="UniformToFill"
												ImageSource="{Binding Icon}" />
										</Border.Background>
										<UIElement.Clip>
											<RectangleGeometry
												RadiusX="16"
												RadiusY="16"
												Rect="0 0 50 50" />
										</UIElement.Clip>
									</Border>
									<StackPanel
										Margin="5,0,5,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Center"
										Orientation="Vertical">
										<TextBlock
											VerticalAlignment="Center"
											FontSize="16"
											FontWeight="Medium"
											Foreground="#FFFFFFFF"
											TextAlignment="Left"
											TextTrimming="CharacterEllipsis"
											ToolTipService.BetweenShowDelay="5"
											Text="{Binding Name}" />
										<TextBlock
											HorizontalAlignment="Left"
											VerticalAlignment="Center"
											FontSize="14"
											FontWeight="Regular"
											Foreground="#FF808080"
											Opacity="0.8"
											TextAlignment="Left"
											TextTrimming="CharacterEllipsis"
											Text="{Binding ExeDir}"
											ToolTip="{Binding ExeDir}" />
									</StackPanel>
								</StackPanel>
								<Grid
									Name="AddButton"
									Column="1"
									Margin="0"
									Opacity="0">
									<StackPanel
										Margin="20,0,20,0"
										HorizontalAlignment="Center"
										VerticalAlignment="Center"
										Orientation="Horizontal">
										<Grid
											HorizontalAlignment="Center"
											VerticalAlignment="Center">
											<Border
												Width="26"
												Height="26"
												Background="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_zBHWQM3TLFWTMTC2_ejd}"
												CornerRadius="13"
												Opacity="0.5" />
											<Image
												Width="15"
												RenderOptions.BitmapScalingMode="Fant"
												Source="{DynamicResource XSvg}" />
										</Grid>
										<TextBlock
											Name="AddGameTextBlock"
											Margin="5,0,0,0"
											VerticalAlignment="Center"
											FontSize="16"
											FontWeight="SemiBold"
											Foreground="#FFFFFFFF"
											Text="{DynamicResource Remove}"
											TextAlignment="Right" />
									</StackPanel>
								</Grid>
							</Grid>
							<ControlTemplate.Triggers>
								<DataTrigger
									Value="True"
									Binding="{Binding IsAdded}">
									<Setter
										TargetName="AddGameTextBlock"
										Value="{StaticResource added}"
										Property="Text" />
									<Setter
										Property="IsEnabled"
										Value="False" />
								</DataTrigger>
								<EventTrigger
									RoutedEvent="MouseEnter">
									<BeginStoryboard>
										<Storyboard>
											<DoubleAnimation
												Storyboard.TargetName="AddButton"
												Storyboard.TargetProperty="Opacity"
												To="1"
												Duration="0:0:0.2" />
										</Storyboard>
									</BeginStoryboard>
								</EventTrigger>
								<EventTrigger
									RoutedEvent="MouseLeave">
									<BeginStoryboard>
										<Storyboard>
											<DoubleAnimation
												Storyboard.TargetName="AddButton"
												Storyboard.TargetProperty="Opacity"
												To="0"
												Duration="0:0:0.2" />
										</Storyboard>
									</BeginStoryboard>
								</EventTrigger>
							</ControlTemplate.Triggers>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style>
		</FrameworkElement.Style>
	</Button>
</liiil1:dje_zXNNJLKZGU3XN9MK85ZPHRMHN6TLT9JSR6D4D7NR9PZJCSNHLEEUA4V5W85KFULPDMQ_ejd>
