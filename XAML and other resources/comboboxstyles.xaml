		liil1il:Key="ComboBoxToggleButton">
		<Setter
			Property="Template">
			<Setter.Value>
				<ControlTemplate
					TargetType="{liil1il:Type ToggleButton}">
					<Grid>
						<Grid.ColumnDefinitions>
							<ColumnDefinition />
							<ColumnDefinition
								Width="14" />
						</Grid.ColumnDefinitions>
						<Border
							Name="Border"
							Grid.ColumnSpan="2"
							Background="{TemplateBinding Background}"
							BorderBrush="#FFF6F6F6"
							BorderThickness="0"
							CornerRadius="7" />
						<Image
							Name="Arrow"
							Grid.Column="1"
							Margin="0,0,5,0"
							HorizontalAlignment="Center"
							VerticalAlignment="Center">
							<Image.Source>
								<DrawingImage>
									<DrawingImage.Drawing>
										<DrawingGroup
											ClipGeometry="M0,0 L0,4 L7,4 L7,0 L0,0">
											<GeometryDrawing
												Brush="#FFFFFFFF"
												Geometry="F1 M7,4 M0,0 M0.8225,0 L3.5,2.47439 L6.1775,0 L7,0.765499 L3.5,4 L0,0.765499 L0.8225,0" />
										</DrawingGroup>
									</DrawingImage.Drawing>
								</DrawingImage>
							</Image.Source>
						</Image>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
