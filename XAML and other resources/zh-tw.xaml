	<ll1:String
		liil1il:Key="NetworkAdapters">網絡適配器</ll1:String>
	<ll1:String
		liil1il:Key="RestartingNetworkAdapters">重新啟動網絡適配器</ll1:String>
	<ll1:String
		liil1il:Key="MinimumShort">最小</ll1:String>
	<ll1:String
