<liiil1:dje_zNRCL9G4XRL4UW4YVEN9US6K5RDEPGK59QWGV6J84YKWMTDVDQY9PKKTRHCFPYAXKMFY55QN2_ejd xmlns:liiil1="clr-namespace:-" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:li1ii="clr-namespace:Material.Icons.WPF;assembly=Material.Icons.WPF" xmlns:liil1il="http://schemas.microsoft.com/winfx/2006/xaml">
	<Grid
		Margin="0,0,0,5">
		<Border
			Background="#FF000000"
			CornerRadius="20"
			Opacity="0.6" />
		<Grid
			Margin="15">
			<Grid.ColumnDefinitions>
				<ColumnDefinition
					Width="*" />
				<ColumnDefinition
					Width="auto" />
				<ColumnDefinition
					Width="auto" />
			</Grid.ColumnDefinitions>
			<Grid.RowDefinitions>
				<RowDefinition
					Height="auto" />
				<RowDefinition
					Height="auto" />
			</Grid.RowDefinitions>
			<StackPanel
				Grid.Column="0"
				Orientation="Horizontal">
				<li1ii:MaterialIcon
					FrameworkElement.Width="23"
					FrameworkElement.Height="23"
					FrameworkElement.Margin="0,0,6,0"
					Control.Foreground="#FFFFFFFF"
					RenderOptions.BitmapScalingMode="Fant"
					MaterialIcon.Kind="{Binding Icon}" />
				<CheckBox
					Name="NameCheckBox"
					Grid.Column="0"
					Foreground="#FFFFFFFF"
					Style="{DynamicResource TweaksButtonStyle}"
					Content="{Binding DisplayName}" />
			</StackPanel>
			<TextBlock
				Grid.Row="0"
				Grid.Column="1"
				Margin="10,0,0,0"
				VerticalAlignment="Center"
				FontSize="15"
				FontWeight="Medium"
				TextAlignment="Right"
				Foreground="{Binding StatusForeground}"
				Opacity="{Binding StatusOpacity}"
				Text="{Binding Status}" />
			<TextBox
				Grid.Row="0"
				Grid.Column="3"
				Width="150"
				Margin="5,0,0,0"
				HorizontalAlignment="Right"
				FontSize="14"
				FontWeight="Medium"
				Foreground="#FF1E1E1E"
				MaxLength="8"
				MaxLines="1"
				Template="{StaticResource TextBoxBaseControlTemplateForTweaks}"
				TextAlignment="Center"
				ToolTipService.BetweenShowDelay="0"
				ToolTipService.InitialShowDelay="0"
				Text="{Binding CustomValue}"
				ToolTip="{Binding TextBoxToolTip}"
				Visibility="{Binding TextBoxVisibility}" />
			<ComboBox
				Grid.Row="0"
				Grid.Column="3"
				Width="150"
				Margin="5,0,0,0"
				HorizontalAlignment="Right"
				Foreground="#FFFFFFFF"
				ItemContainerStyle="{DynamicResource theComboBoxItem}"
				Style="{DynamicResource theComboBox}"
				VirtualizingPanel.IsVirtualizing="True"
				VirtualizingPanel.IsVirtualizingWhenGrouping="True"
				VirtualizingPanel.VirtualizationMode="Recycling"
				ItemsSource="{Binding Values, Mode=OneWay}"
				SelectedItem="{Binding SelectedLabel}"
				Visibility="{Binding ComboBoxVisibility}" />
			<StackPanel
				Grid.Row="2"
				Grid.Column="0"
				Grid.ColumnSpan="3"
				Margin="0,10,0,0"
				Opacity="0"
				Visibility="Collapsed">
				<TextBlock
					Margin="0,0,0,0"
					FontSize="16"
					FontWeight="Regular"
					Foreground="#FFDDDDDD"
					TextWrapping="Wrap"
					Text="{Binding DescriptionShort}" />
				<Grid
					Height="20"
					Margin="0,10,0,0"
					HorizontalAlignment="Left">
					<Border
						Background="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z82W2KBY2W2H2UZX34LHGR_ejd}"
						CornerRadius="10" />
					<CheckBox
						Name="ShowFullDescription"
						Margin="5,2,8,0"
						Content="{DynamicResource FULLDESCRIPTION}"
						Foreground="#FFFFFFFF"
						Style="{DynamicResource DesriptionButtonStyle}"
						IsChecked="{Binding ShowFullDescription}" />
				</Grid>
				<StackPanel
					Margin="0,10,0,0"
					Opacity="0"
					Visibility="Collapsed">
					<TextBlock
						FontSize="16"
						FontWeight="Regular"
						Foreground="#FFDDDDDD"
						TextWrapping="Wrap"
						Text="{Binding DescriptionLong}" />
					<FrameworkElement.Style>
						<Style
							TargetType="{liil1il:Type StackPanel}">
							<Style.Triggers>
								<DataTrigger
									Value="True"
									Binding="{Binding ElementName=ShowFullDescription, Path=IsChecked}">
									<TriggerBase.EnterActions>
										<BeginStoryboard>
											<Storyboard>
												<DoubleAnimation
													BeginTime="0:0:0.2"
													Storyboard.TargetProperty="Opacity"
													From="0"
													To="1"
													Duration="0:0:0.2" />
												<ObjectAnimationUsingKeyFrames
													Storyboard.TargetProperty="Visibility">
													<DiscreteObjectKeyFrame
														KeyTime="0:0:0.02"
														Value="{liil1il:Static Visibility.Visible}" />
												</ObjectAnimationUsingKeyFrames>
											</Storyboard>
										</BeginStoryboard>
									</TriggerBase.EnterActions>
									<TriggerBase.ExitActions>
										<BeginStoryboard>
											<Storyboard>
												<DoubleAnimation
													Storyboard.TargetProperty="Opacity"
													From="1"
													To="0"
													Duration="0:0:0.2" />
												<ObjectAnimationUsingKeyFrames
													BeginTime="0:0:0.2"
													Storyboard.TargetProperty="Visibility">
													<DiscreteObjectKeyFrame
														KeyTime="0:0:0.02"
														Value="{liil1il:Static Visibility.Collapsed}" />
												</ObjectAnimationUsingKeyFrames>
											</Storyboard>
										</BeginStoryboard>
									</TriggerBase.ExitActions>
								</DataTrigger>
							</Style.Triggers>
						</Style>
					</FrameworkElement.Style>
				</StackPanel>
				<Grid
					Margin="0,10,0,0">
					<Border
						Background="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z7Y3AEKDZ_ejd}"
						CornerRadius="20"
						Opacity="0.3" />
					<Border
						BorderBrush="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z7Y3AEKDZ_ejd}"
						BorderThickness="4"
						CornerRadius="20" />
					<StackPanel
						Margin="15">
						<TextBlock
							FontSize="12"
							FontWeight="Bold"
							Foreground="#FFDDDDDD"
							Text="{DynamicResource RECOMENDATION}"
							TextWrapping="Wrap" />
						<TextBlock
							Margin="0,5,0,0"
							FontSize="16"
							FontWeight="Regular"
							Foreground="#FFDDDDDD"
							TextWrapping="Wrap"
							Text="{Binding Recommendation}" />
						<TextBlock
							Margin="0,8,0,0"
							FontSize="16"
							FontWeight="Regular"
							Foreground="#FFDDDDDD"
							TextWrapping="Wrap"
							Text="{Binding Consider}"
							Visibility="{Binding ConsiderVisibility}" />
					</StackPanel>
				</Grid>
				<FrameworkElement.Style>
					<Style
						TargetType="{liil1il:Type StackPanel}">
						<Style.Triggers>
							<DataTrigger
								Value="True"
								Binding="{Binding ElementName=NameCheckBox, Path=IsChecked}">
								<TriggerBase.EnterActions>
									<BeginStoryboard>
										<Storyboard>
											<DoubleAnimation
												BeginTime="0:0:0.2"
												Storyboard.TargetProperty="Opacity"
												From="0"
												To="1"
												Duration="0:0:0.2" />
											<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="Visibility">
												<DiscreteObjectKeyFrame
													KeyTime="0:0:0.02"
													Value="{liil1il:Static Visibility.Visible}" />
											</ObjectAnimationUsingKeyFrames>
										</Storyboard>
									</BeginStoryboard>
								</TriggerBase.EnterActions>
								<TriggerBase.ExitActions>
									<BeginStoryboard>
										<Storyboard>
											<DoubleAnimation
												Storyboard.TargetProperty="Opacity"
												From="1"
												To="0"
												Duration="0:0:0.2" />
											<ObjectAnimationUsingKeyFrames
												BeginTime="0:0:0.2"
												Storyboard.TargetProperty="Visibility">
												<DiscreteObjectKeyFrame
													KeyTime="0:0:0.02"
													Value="{liil1il:Static Visibility.Collapsed}" />
											</ObjectAnimationUsingKeyFrames>
										</Storyboard>
									</BeginStoryboard>
								</TriggerBase.ExitActions>
							</DataTrigger>
						</Style.Triggers>
					</Style>
				</FrameworkElement.Style>
			</StackPanel>
		</Grid>
	</Grid>
</liiil1:dje_zNRCL9G4XRL4UW4YVEN9US6K5RDEPGK59QWGV6J84YKWMTDVDQY9PKKTRHCFPYAXKMFY55QN2_ejd>
