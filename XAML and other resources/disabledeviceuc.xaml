						<Style
							TargetType="{liil1il:Type TextBlock}">
							<Style.Triggers>
								<DataTrigger
									Value="True"
									Binding="{Binding ElementName=NameCheckBox, Path=IsChecked}">
									<TriggerBase.EnterActions>
										<BeginStoryboard>
											<Storyboard>
												<DoubleAnimation
													BeginTime="0:0:0.2"
													Storyboard.TargetProperty="Opacity"
