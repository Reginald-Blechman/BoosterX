										Opacity="0.1" />
									<TextBlock
										Grid.Row="0"
										Grid.Column="0"
										Margin="15"
										VerticalAlignment="Center"
										FontSize="16"
										FontWeight="Regular"
										Foreground="#FFDDDDDD"
										Text="{DynamicResource StrippedSteamDesc2}"
										TextAlignment="Left"
										TextWrapping="Wrap" />
								</Grid>
								<TextBlock
									Margin="0,10,0,0"
									VerticalAlignment="Center"
									FontFamily="/Resources/Fonts/#LexendExa"
									FontSize="16"
									FontWeight="Medium"
									Foreground="#FFFFFFFF"
									Text="{DynamicResource SteamOneAccSupport}"
									TextAlignment="Right"
									TextWrapping="Wrap" />
								<StackPanel
									Margin="0,10,0,0"
									HorizontalAlignment="Right"
									Orientation="Horizontal">
									<TextBlock
										Name="AddedTextBlock"
										Margin="0,0,15,0"
										VerticalAlignment="Center"
										FontFamily="/Resources/Fonts/#LexendExa"
										FontSize="15"
										FontWeight="Bold"
										Foreground="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z7Y3AEKDZ_ejd}"
										Opacity="0"
										Text="{DynamicResource added}"
										TextAlignment="Left" />
									<Button
										Width="150"
										Height="40"
										Content="{DynamicResource Create}"
										Style="{DynamicResource MediumRoundedGradientButton}" />
								</StackPanel>
								<TextBlock
									Margin="0,15,0,0"
									VerticalAlignment="Center"
									FontFamily="/Resources/Fonts/#LexendExa"
									FontSize="16"
									FontWeight="Medium"
									Foreground="#FFFFFFFF"
									Text="{DynamicResource SteamOneAccSupport2}"
									TextAlignment="Right"
									TextWrapping="Wrap" />
