<liiil1:dje_z7GQ8CY5Z2XS4Y64HBD4P8B5PHSDDDFUF6BZCQG89LBTBJZUD4P5V5B2PR8ESF49VB2_ejd xmlns:liiil1="clr-namespace:-" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:liil1il="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:li1ii="clr-namespace:Material.Icons.WPF;assembly=Material.Icons.WPF">
	<Grid>
		<Border
			Grid.ColumnSpan="4"
			Background="#FF000000"
			CornerRadius="20"
			Opacity="0.6" />
		<Grid
			Margin="15">
			<Grid.RowDefinitions>
				<RowDefinition />
				<RowDefinition />
			</Grid.RowDefinitions>
			<Grid.ColumnDefinitions>
				<ColumnDefinition
					Width="auto" />
				<ColumnDefinition
					Width="*" />
			</Grid.ColumnDefinitions>
			<StackPanel
				Orientation="Horizontal">
				<TextBox
					Name="NameTB"
					Grid.Column="0"
					Margin="0,0,0,0"
					Padding="5,2,5,2"
					HorizontalAlignment="Left"
					VerticalAlignment="Center"
					Background="#00FFFFFF"
					CaretBrush="#FFFFFFFF"
					Cursor="IBeam"
					FontSize="16"
					FontWeight="Regular"
					Foreground="#FFFFFFFF"
					Template="{DynamicResource ComboColoredTextBoxTemplate}"
					Text="{Binding Name, Mode=TwoWay}" />
				<Button
					Name="OKButton"
					Margin="10,0,0,0"
					Content="OK"
					Opacity="0"
					Visibility="Collapsed"
					Command="{Binding Rename}">
					<FrameworkElement.Style>
						<Style
							BasedOn="{StaticResource MediumRoundedButtonBlue}"
							TargetType="{liil1il:Type Button}">
							<Style.Triggers>
								<DataTrigger
									Value="True"
									Binding="{Binding ElementName=NameTB, Path=IsFocused}">
									<TriggerBase.EnterActions>
										<BeginStoryboard>
											<Storyboard>
												<DoubleAnimation
													BeginTime="0:0:0.2"
													Storyboard.TargetProperty="Opacity"
													From="0"
													To="1"
													Duration="0:0:0.2" />
												<ObjectAnimationUsingKeyFrames
													Storyboard.TargetProperty="Visibility">
													<DiscreteObjectKeyFrame
														KeyTime="0:0:0.02"
														Value="{liil1il:Static Visibility.Visible}" />
												</ObjectAnimationUsingKeyFrames>
											</Storyboard>
										</BeginStoryboard>
									</TriggerBase.EnterActions>
									<TriggerBase.ExitActions>
										<BeginStoryboard>
											<Storyboard>
												<DoubleAnimation
													Storyboard.TargetProperty="Opacity"
													From="1"
													To="0"
													Duration="0:0:0.2" />
												<ObjectAnimationUsingKeyFrames
													BeginTime="0:0:0.2"
													Storyboard.TargetProperty="Visibility">
													<DiscreteObjectKeyFrame
														KeyTime="0:0:0.02"
														Value="{liil1il:Static Visibility.Collapsed}" />
												</ObjectAnimationUsingKeyFrames>
											</Storyboard>
										</BeginStoryboard>
									</TriggerBase.ExitActions>
								</DataTrigger>
							</Style.Triggers>
						</Style>
					</FrameworkElement.Style>
				</Button>
			</StackPanel>
			<TextBlock
				Grid.Row="1"
				Grid.Column="0"
				Margin="0,0,0,0"
				HorizontalAlignment="Left"
				VerticalAlignment="Center"
				FontSize="16"
				FontWeight="Regular"
				Foreground="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_zBHWQM3TLFWTMTC2_ejd}"
				Opacity="0.5"
				TextTrimming="CharacterEllipsis"
				Text="{Binding DateCreated}" />
			<StackPanel
				Grid.Row="0"
				Grid.RowSpan="2"
				Grid.Column="1"
				Height="40"
				HorizontalAlignment="Right"
				VerticalAlignment="Center"
				Orientation="Horizontal">
				<TextBlock
					Margin="10,0,10,0"
					HorizontalAlignment="Left"
					VerticalAlignment="Center"
					FontSize="16"
					FontWeight="Regular"
					TextTrimming="CharacterEllipsis"
					Foreground="{Binding StatusColor}"
					Opacity="{Binding StatusOpacity}"
					Text="{Binding StatusText}" />
				<Button
					Margin="10,0,10,0"
					Style="{DynamicResource BorderOutlineButtonStyle}"
					ToolTip="{DynamicResource Delete}"
					ToolTipService.BetweenShowDelay="100"
					ToolTipService.InitialShowDelay="100"
					Command="{Binding Delete}">
					<li1ii:MaterialIcon
						FrameworkElement.Margin="5"
						Control.Foreground="#FFFFFFFF"
						MaterialIcon.Kind="TrashCanOutline" />
				</Button>
				<Button
					Margin="10,0,10,0"
					Style="{DynamicResource BorderOutlineButtonStyle}"
					ToolTip="{DynamicResource OpenLocation}"
					ToolTipService.BetweenShowDelay="100"
					ToolTipService.InitialShowDelay="100"
					Command="{Binding OpenLocation}">
					<li1ii:MaterialIcon
						FrameworkElement.Margin="5"
						Control.Foreground="#FFFFFFFF"
						MaterialIcon.Kind="FolderOutline" />
				</Button>
				<Button
					Margin="10,0,10,0"
					Style="{DynamicResource BorderOutlineButtonStyle}"
					ToolTip="{DynamicResource LoadToCloud}"
					ToolTipService.BetweenShowDelay="100"
					ToolTipService.InitialShowDelay="100"
					Command="{Binding UploadToCloud}">
					<li1ii:MaterialIcon
						FrameworkElement.Margin="5"
						Control.Foreground="#FFFFFFFF"
						MaterialIcon.Kind="CloudArrowUpOutline" />
				</Button>
				<Button
					Margin="10,5,0,5"
					Content="{DynamicResource Open}"
					Style="{DynamicResource MediumRoundedButtonBlue}"
					Command="{Binding Choose}" />
			</StackPanel>
		</Grid>
	</Grid>
</liiil1:dje_z7GQ8CY5Z2XS4Y64HBD4P8B5PHSDDDFUF6BZCQG89LBTBJZUD4P5V5B2PR8ESF49VB2_ejd>
