<liiil1:dje_zZA5YXNN85RQEDRBPZE7A5M2UPE75H2DGEEV285VQRKVHWGHJYZ39FVL9MBDA_ejd xmlns:liiil1="clr-namespace:-" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:liil1il="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:li1ii1="clr-namespace:Microsoft.Xaml.Behaviors;assembly=Microsoft.Xaml.Behaviors"
	Page.Title="NvidiaPage">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition
				Height="auto" />
			<RowDefinition
				Height="auto" />
			<RowDefinition
				Height="*" />
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition
				Width="*" />
		</Grid.ColumnDefinitions>
		<Grid
			Row="0"
			Column="0"
			Width="625"
			HorizontalAlignment="Left">
			<Border
				Background="#FF000000"
				CornerRadius="20"
				Opacity="0.6" />
			<StackPanel
				Margin="6.5">
				<StackPanel
					Margin="0,0,0,0"
					HorizontalAlignment="Stretch"
					VerticalAlignment="Center"
					Orientation="Horizontal">
					<Button
						Name="RevertButton"
						Width="150"
						Height="36"
						Margin="0,0,0,0"
						Background="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z2B5DBTEJ9B4VE9Z_ejd}"
						Style="{DynamicResource TweakingTopButton}"
						ToolTip="{DynamicResource NvDefDesc}"
						ToolTipService.BetweenShowDelay="100"
						ToolTipService.InitialShowDelay="100"
						Command="{Binding ResetProfileClickCommand}">
						<ContentControl.Content>
							<StackPanel
								Orientation="Horizontal">
								<Image
									Width="15"
									Margin="0,0,8,0"
									Source="{StaticResource SpeedometerLow}" />
								<TextBlock
									HorizontalAlignment="Center"
									VerticalAlignment="Center"
									FontSize="16"
									FontWeight="Regular"
									Foreground="#FFFFFFFF"
									Text="{DynamicResource default}" />
							</StackPanel>
						</ContentControl.Content>
					</Button>
					<Button
						Name="OptimalButton"
						Width="150"
						Height="36"
						Margin="4,0,0,0"
						Background="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z7Y3AEKDZ_ejd}"
						Style="{DynamicResource TweakingTopButton}"
						Command="{Binding OptimalClickCommand}">
						<ContentControl.Content>
							<StackPanel
								Orientation="Horizontal">
								<Image
									Width="15"
									Margin="0,0,8,0"
									Source="{StaticResource SpeedometerMiddle}" />
								<TextBlock
									HorizontalAlignment="Center"
									VerticalAlignment="Center"
									FontSize="16"
									FontWeight="Regular"
									Foreground="#FFFFFFFF"
									Text="{DynamicResource optimal}" />
							</StackPanel>
						</ContentControl.Content>
					</Button>
					<Border
						Width="150"
						Height="36"
						Margin="4,0,0,0"
						Background="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z2B5DBTEJ9B4VE9Z_ejd}"
						CornerRadius="14">
						<StackPanel
							HorizontalAlignment="Center"
							Orientation="Horizontal">
							<Button
								Name="ImportButton"
								Width="15"
								Margin="0,0,28,0"
								Style="{DynamicResource InvisibleButton}"
								ToolTip="{DynamicResource Import}"
								ToolTipService.BetweenShowDelay="200"
								ToolTipService.InitialShowDelay="200"
								Command="{Binding ImportClickCommand}">
								<Image
									Source="{DynamicResource ImportSvg}" />
							</Button>
							<Border
								Width="2"
								Height="30"
								Background="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z82W2KBY2W2H2UZX34LHGR_ejd}"
								CornerRadius="1" />
							<Button
								Name="ExportButton"
								Width="15"
								Margin="28,0,0,0"
								Style="{DynamicResource InvisibleButton}"
								ToolTip="{DynamicResource Export}"
								ToolTipService.BetweenShowDelay="200"
								ToolTipService.InitialShowDelay="200"
								Command="{Binding ExportClickCommand}">
								<Image
									Source="{DynamicResource ExportSvg}" />
							</Button>
						</StackPanel>
					</Border>
					<Button
						Name="RefreshButton"
						Width="150"
						Height="36"
						Margin="4,0,0,0"
						HorizontalAlignment="Right"
						Background="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z2B5DBTEJ9B4VE9Z_ejd}"
						Style="{DynamicResource TweakingTopButton}"
						Command="{Binding RefreshCommand}">
						<ContentControl.Content>
							<StackPanel
								Orientation="Horizontal">
								<Image
									Width="15"
									Margin="0,0,8,0"
									Source="{StaticResource RefreshIcon}" />
								<TextBlock
									HorizontalAlignment="Center"
									VerticalAlignment="Center"
									FontSize="16"
									FontWeight="Regular"
									Foreground="#FFFFFFFF"
									Text="{DynamicResource update}" />
							</StackPanel>
						</ContentControl.Content>
					</Button>
				</StackPanel>
				<Grid
					Margin="0,6.5,0,0">
					<StackPanel>
						<Grid
							VerticalAlignment="Center">
							<Grid.ColumnDefinitions>
								<ColumnDefinition
									Width="300" />
								<ColumnDefinition
									Width="*" />
							</Grid.ColumnDefinitions>
							<Grid
								Margin="0,0,5,0">
								<ComboBox
									Name="GamesCombo"
									Width="300"
									Height="0"
									Margin="0,0,0,0"
									HorizontalAlignment="Left"
									VerticalAlignment="Bottom"
									Focusable="False"
									Foreground="#00FFFFFF"
									IsTextSearchEnabled="True"
									ItemContainerStyle="{DynamicResource theComboBoxItem}"
									MaxDropDownHeight="200"
									Style="{DynamicResource theComboBoxInvisible2}"
									SelectedItem="{Binding CurrentProfile, Mode=OneWay}">
									<ItemsControl.ItemsPanel>
										<ItemsPanelTemplate>
											<VirtualizingStackPanel />
										</ItemsPanelTemplate>
									</ItemsControl.ItemsPanel>
									<ItemsControl.ItemTemplate>
										<DataTemplate>
											<TextBlock
												Focusable="False"
												FontSize="16"
												FontWeight="Medium"
												Foreground="#FFFFFFFF"
												Text="{Binding}" />
										</DataTemplate>
									</ItemsControl.ItemTemplate>
									<li1ii1:Interaction.Triggers>
										<li1ii1:EventTrigger
											EventTrigger.EventName="SelectionChanged">
											<li1ii1:InvokeCommandAction
												InvokeCommandAction.Command="{Binding OnProfileChangedCommand}"
												InvokeCommandAction.CommandParameter="{Binding SelectedItem, RelativeSource={RelativeSource AncestorType=ComboBox, Mode=FindAncestor}}" />
										</li1ii1:EventTrigger>
									</li1ii1:Interaction.Triggers>
								</ComboBox>
								<Border
									Background="#FF508BFF"
									CornerRadius="15"
									Opacity="0.15" />
								<TextBox
									Name="lll"
									Width="300"
									Height="36"
									Padding="12,0,0,0"
									HorizontalContentAlignment="Left"
									VerticalContentAlignment="Center"
									Background="#00FFFFFF"
									CaretBrush="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_zBHWQM3TLFWTMTC2_ejd}"
									FontSize="16"
									FontWeight="Regular"
									Foreground="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_zBHWQM3TLFWTMTC2_ejd}"
									Template="{StaticResource ComboColoredTextBoxTemplate2}"
									Text="{DynamicResource SearchGameForSetup}" />
							</Grid>
							<TextBlock
								Grid.Column="1"
								MaxWidth="320"
								Margin="0,0,0,0"
								HorizontalAlignment="Center"
								VerticalAlignment="Center"
								FontSize="16"
								FontWeight="Medium"
								TextTrimming="CharacterEllipsis"
								TextWrapping="Wrap"
								Foreground="{Binding StatusColor}"
								Opacity="{Binding StatusOpacity}"
								Text="{Binding StatusText}" />
						</Grid>
					</StackPanel>
				</Grid>
			</StackPanel>
		</Grid>
		<liiil1:dje_zXNNJLKZGU3XN9MK85ZPHRMHN6E5ELSTJG7NE358T5V8U64Z_ejd
			Grid.Row="2"
			FrameworkElement.VerticalAlignment="Top"
			UIElement.Opacity="{Binding AnimationOpacity}"
			UIElement.Visibility="{Binding AnimationVisibility}" />
		<StackPanel
			Grid.Row="1"
			Margin="0,10,0,0">
			<StackPanel
				HorizontalAlignment="Left"
				Orientation="Horizontal">
				<CheckBox
					Name="TipCheckBox"
					Content="{DynamicResource Tip}"
					Style="{DynamicResource TipCheckBoxStyle}" />
			</StackPanel>
			<StackPanel
				Margin="10,5,10,10"
				Opacity="0"
				Visibility="Collapsed">
				<StackPanel
					Orientation="Horizontal">
					<TextBlock
						HorizontalAlignment="Center"
						VerticalAlignment="Center"
						FontSize="16"
						FontWeight="SemiBold"
						Foreground="#FFFFFFFF"
						Text="{DynamicResource RecommendedToClick}" />
					<TextBlock
						Margin="5,0,0,0"
						HorizontalAlignment="Center"
						VerticalAlignment="Center"
						FontSize="16"
						FontWeight="SemiBold"
						Foreground="{liil1il:Static liiil1:dje_zG25CMVCNLAXR669QGY4VQH2TVB3HB4JJZQ_ejd.dje_z7Y3AEKDZ_ejd}"
						Text="{DynamicResource optimal}" />
				</StackPanel>
				<TextBlock
					Margin="0,10,0,0"
					HorizontalAlignment="Left"
					VerticalAlignment="Center"
					FontSize="14"
					FontWeight="Regular"
					Foreground="#FFFFFFFF"
					Opacity="0.9"
					Text="{DynamicResource NvidiaRecom}"
					TextWrapping="Wrap" />
				<FrameworkElement.Style>
					<Style
						TargetType="{liil1il:Type StackPanel}">
						<Style.Triggers>
							<DataTrigger
								Value="True"
								Binding="{Binding ElementName=TipCheckBox, Path=IsChecked}">
								<TriggerBase.EnterActions>
									<BeginStoryboard>
										<Storyboard>
											<DoubleAnimation
												BeginTime="0:0:0.2"
												Storyboard.TargetProperty="Opacity"
												From="0"
												To="1"
												Duration="0:0:0.2" />
											<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="Visibility">
												<DiscreteObjectKeyFrame
													KeyTime="0:0:0.02"
													Value="{liil1il:Static Visibility.Visible}" />
											</ObjectAnimationUsingKeyFrames>
										</Storyboard>
									</BeginStoryboard>
								</TriggerBase.EnterActions>
								<TriggerBase.ExitActions>
									<BeginStoryboard>
										<Storyboard>
											<DoubleAnimation
												Storyboard.TargetProperty="Opacity"
												From="1"
												To="0"
												Duration="0:0:0.2" />
											<ObjectAnimationUsingKeyFrames
												BeginTime="0:0:0.2"
												Storyboard.TargetProperty="Visibility">
												<DiscreteObjectKeyFrame
													KeyTime="0:0:0.02"
													Value="{liil1il:Static Visibility.Collapsed}" />
											</ObjectAnimationUsingKeyFrames>
										</Storyboard>
									</BeginStoryboard>
								</TriggerBase.ExitActions>
							</DataTrigger>
						</Style.Triggers>
					</Style>
				</FrameworkElement.Style>
			</StackPanel>
		</StackPanel>
		<liiil1:dje_zAS6WZLBNFP8GLLU4MJMXKNYQDS7A4HAJMJP568D8XGRL3AQ_ejd
			Grid.Row="2"
			FrameworkElement.Margin="0,10,0,0"
			FrameworkElement.MaxHeight="{Binding Path=ActualHeight, RelativeSource={RelativeSource AncestorType=Grid}}"
			FrameworkElement.DataContext="{Binding}"
			UIElement.Opacity="{Binding MainOpacity}" />
	</Grid>
</liiil1:dje_zZA5YXNN85RQEDRBPZE7A5M2UPE75H2DGEEV285VQRKVHWGHJYZ39FVL9MBDA_ejd>
